pkgname=seabios-bin
pkgver=1.16.0
pkgrel=1
pkgdesc="A 16-bit x86 bios"
arch=('any')
url="https://www.coreboot.org/SeaBIOS"
license=('GPL3' 'LGPL3')
provides=('seabios')
options=('!makeflags' '!strip')
conflicts=('seabios')
source=(https://mirror.f4st.host/archlinux/extra/os/x86_64/seabios-1.16.0-1-any.pkg.tar.zst)
sha1sums=('e3f41d44e193a8654544c44d69484633bf233f53')

package() {
  install -Dm644 -t "${pkgdir}/usr/share/qemu" usr/share/qemu/*.bin
}
