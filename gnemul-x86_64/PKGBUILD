pkgname=gnemul-x86_64
pkgver=1.1.3
_pkgdate=20220210
pkgrel=1
pkgdesc="x86_64 runtime libraries for qemu-user"
arch=('x86_64' 'loongarch64')
url="http://loongnix.cn"
license=('BSD')
depends=('gcc-libs')
source=(http://pkg.loongnix.cn/loongnix/pool/main/i/i386-runtime/i386-runtime_${pkgver}~${_pkgdate}.lnd.1_loongarch64.deb)
sha256sums=('2cd64cadda97135d8f613c0f3692c6d186c98be8b9d4e802b598bd6b9c4bd7d5')
OPTIONS=(!strip)

package() {
  tar xf data.tar.xz
  mkdir -p ${pkgdir}/usr/gnemul
  cp -r usr/gnemul/latx-x86_64/ ${pkgdir}/usr/gnemul/qemu-x86_64
}
